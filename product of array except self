class Solution(object):
    def productExceptSelf(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        n=len(nums)
        k=[1]*n
        p=1
        for i in range(n):
            k[i]=p
            p*=nums[i]
        s=1
        for j in range(n-1,-1,-1):
            k[j]*=s
            s*=nums[j]
        
        return k
